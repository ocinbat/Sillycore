{
  "AppSettings": {
    "Test": "test"
  },
  "Sillycore": {
    "Paging": {
      "DefaultPageSize": 12
    },
    "Swagger": {
      "Title": "WebApplication",
      "Version": "v1.0",
      "Description": "Sample web api application to demonstrate capabilities of Sillycore.",
      "TermsOfService": "",
      "Contact": {
        "Email": "omer@cinbat.com",
        "Name": "Omer Cinbat",
        "Url": "http://cinbat.com"
      },
      "License": {
        "Name": "MIT",
        "Url": "https://raw.githubusercontent.com/ocinbat/Sillycore/master/LICENSE"
      }
    },
    "Web": {
      "StaticFiles": {
        "ServeUnknownFileTypes": true,
        "DefaultContentType": "text/plain"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "autoRegisterTemplate": true
        }
      }
    ]
  },
  "Sillycore.DockerImageName": "omertest",
  "AuthServer": "https://google.com",
  "MetricsOptions": {
    "DefaultContextLabel": "SilliCoreWebApp",
    "Enabled": true
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.1,
    "IgnoredHttpStatusCodes": [
      404
    ],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true,
    "EnvironmentInfoEndpointEnabled": true
  },
  "MetricsElasticSearchOptions": {
    "Enabled": false,
    "BaseUri": "",
    "Index": "metrics",
    "FlushIntervalInSeconds": 5
  },
  "ApplicationInsights": {
    "InstrumentationKey": "key"
  }
}